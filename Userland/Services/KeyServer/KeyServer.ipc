endpoint KeyServer
{
    add_username_password([UTF8] String id, [UTF8] String username, [UTF8] String password) => (bool success)
    get_username_password([UTF8] String id) => (bool success, bool exists, [UTF8] String username, [UTF8] String password)

    add_key([UTF8] String id, [UTF8] String key) => (bool success)
    get_key([UTF8] String id) => (bool success, bool exists, [UTF8] String key)
}
